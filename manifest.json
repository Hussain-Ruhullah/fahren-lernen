{
	"manifest_version": 3,
	"name": "Fahren-Lernen",
	"description": "chrome extension for keyboard navigation for Fahre-Lernen.de by Hussain Ruhullah",
	"version": "1.0.0",
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"content_scripts": [
		{
			"js": [
				"scripts/app.js"
			],
			"matches": [
				"https://html.fahren-lernen.de/*"
			]
		}
	]
}